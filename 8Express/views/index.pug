<doctype html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title GYM Membership
    
       style
           style
            include ../style.css
 
    style
    
  body
    nav
      ul
        li
          a(href="/") Home
        li
          a(href="/") About
        li
          a(href="/") Contact
    .container
      h1 Get this GYM Membership
      form(action="/" method="post" id="contact")
        label(for="name") Enter Your Name
        input(type="text" name="name" id="name" placeholder="Enter Your Name")
        label(for="age") Enter Your Age
        input(type="text" name="age" id="age" placeholder="Enter Your Age")
        label(for="gender") Enter Your Gender
        input(type="text" name="gender" id="gender" placeholder="Enter Your Gender")
        label(for="address") Enter Your Address
        textarea(name="address" id="address" cols="30" rows="10" placeholder="Enter Your Address")
        label(for="more") More About You
        textarea(name="more" id="more" cols="30" rows="10" placeholder="More About You")
        input(type="submit" value="Submit")
